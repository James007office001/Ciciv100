{
    "memory":  "512",
    "executionRoleArn":  "arn:aws:iam::244977608002:role/ecsTaskExecutionRole",
    "family":  "c2-minimal",
    "networkMode":  "awsvpc",
    "requiresCompatibilities":  [
                                    "FARGATE"
                                ],
    "containerDefinitions":  [
                                 {
                                     "portMappings":  [
                                                          {
                                                              "protocol":  "tcp",
                                                              "containerPort":  3000
                                                          }
                                                      ],
                                     "logConfiguration":  {
                                                              "logDriver":  "awslogs",
                                                              "options":  {
                                                                              "awslogs-region":  "us-east-1",
                                                                              "awslogs-stream-prefix":  "c2-minimal",
                                                                              "awslogs-group":  "/ecs/c2-backend"
                                                                          }
                                                          },
                                     "name":  "c2-app",
                                     "essential":  true,
                                     "image":  "244977608002.dkr.ecr.us-east-1.amazonaws.com/c2-backend:latest"
                                 }
                             ],
    "cpu":  "256"
}
