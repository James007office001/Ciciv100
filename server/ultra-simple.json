{
    "memory":  "512",
    "executionRoleArn":  "arn:aws:iam::244977608002:role/ecsTaskExecutionRole",
    "family":  "c2-ultra-simple",
    "networkMode":  "awsvpc",
    "requiresCompatibilities":  [
                                    "FARGATE"
                                ],
    "containerDefinitions":  [
                                 {
                                     "logConfiguration":  {
                                                              "logDriver":  "awslogs",
                                                              "options":  {
                                                                              "awslogs-region":  "us-east-1",
                                                                              "awslogs-stream-prefix":  "hello-world",
                                                                              "awslogs-group":  "/ecs/c2-backend"
                                                                          }
                                                          },
                                     "name":  "simple-container",
                                     "essential":  true,
                                     "image":  "hello-world"
                                 }
                             ],
    "cpu":  "256"
}
